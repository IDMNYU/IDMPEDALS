				"box" : 				{
					"code" : "pink(s)\n{\n\tHistory f0, f1, f2, f3, f4, f5, f6, output;\n\twhite = noise();\n\t\n\tf0 = 0.99886 * f0 + 0.0555179 * white;\n    f1 = 0.99332 * f1 + 0.0750759 * white;\n    f2 = 0.96900 * f2 + 0.1538520 * white;\n    f3 = 0.86650 * f3 + 0.3104856 * white;\n    f4 = 0.55000 * f4 + 0.5329522 * white;\n    f5 = -0.7616 * f5 - 0.0168980 * white;\n    p = f0 + f1 + f2 + f3 + f4 + f5 + f6 + white * 0.5362;\n    p *= 0.144444;\n    f6 = white * 0.115926;\n    outp = p*s + white*(1.0-s);\n\n\treturn outp;\n}\r\n\r\n//\r\n// MAIN CODE HERE:\r\n//\r\n\r\nphase = in1;\r\nshape = in2;\r\nmode = in3;\r\nval = phase; // copy in case proc bypass\r\n\r\nif(mode==0) // sine, clip to square\n{\n\tval = cycle(phase, index=\"phase\");\n\tval *= scale(shape, 0., 1., 1., 10.);\n\tval = clip(val, -1., 1.);\n}\nelse if(mode==1) // sine, waveshape to saw\n{\n\tval = (phase*2.-1.)*shape + cos(phase*TWOPI+PI/2)*(1.0-shape);\n}\nelse if(mode==2) // square, pulse width\n{\n\tshape = clip(shape,0.01, 0.99);\n\tval = (phase>shape)*2.-1.;\n}\nelse if(mode==3) // random steps, noise shape\n{\n\trand = pink(shape);\n\tval = sah(rand, phase, 0.5);\n}\r\nelse if(mode==4) // dust, noise shape\n{\n\tp = rate(phase, 1/16);\n\trand = pink(shape);\n\tclick = change(sah(rand, p, 0.5)>0.5);\n\tval = (click==1)*2-1.;\n\t//val = click;\n}\nelse if(mode==5) // random ramps, noise shape\n{\n\trand = pink(shape);\n\tslope = 1000; // slew\n\tval = slide(sah(rand, phase, 0.5), slope, slope);\n}\nelse if(mode==6) // triangle, exp/lin/log\n{\n\tval = triangle(phase, 0.5);\n\tlogval = sqrt(val);\n\texpval = 1.0-sqrt(1.0-val);\n\tval = shape < 0.5 ? mix(expval, val, shape*2) : mix(val, logval, (shape-0.5)*2);\n\tval = val*2.-1.;\n}\nelse if(mode==7) // rising saw, exp/lin/log\n{\n\tval = triangle(phase, 1.0);\n\tlogval = sqrt(val);\n\texpval = 1.0-sqrt(1.0-val);\n\tval = shape < 0.5 ? mix(expval, val, shape*2) : mix(val, logval, (shape-0.5)*2);\n\tval = val*2.-1.;\n}\nelse if(mode==8) // falling saw, exp/lin/log\n{\n\tval = triangle(phase, 0.);\n\tlogval = sqrt(val);\n\texpval = 1.0-sqrt(1.0-val);\n\tval = shape < 0.5 ? mix(expval, val, shape*2) : mix(val, logval, (shape-0.5)*2);\n\tval = val*2.-1.;\n}\r\n\r\nout1 = val;\r\n\r\n\r\n\r\n",
